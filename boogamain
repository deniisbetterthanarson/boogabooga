	FarmSection:NewToggle("Afk Eat Bluefruit", "Click to toggle", function(state)
    if state then
        _G.autoEat = true;
	while _G.autoEat == true do
	local args = {[1] = "Bluefruit"}
	game:GetService("ReplicatedStorage").Events.UseBagItem:FireServer(unpack(args))
    wait(15)
end
    else
        _G.autoEat = false;
	while _G.autoEat == true do
	local args = {[1] = "Bluefruit"}
	game:GetService("ReplicatedStorage").Events.UseBagItem:FireServer(unpack(args))
    wait(15)
end
    end
end)

